{
    "name": "tarioch/eveapi-fetcher-bundle",
    "description": "Provides automatic EVE Online Api fetching to your Symfony2 project.",
    "keywords": ["EVE Online", "EVE API", "Symfony"],
    "homepage": "https://github.com/tarioch/eveapi-fetcher-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
		"name": "Talos Katuma",
		"homepage": "https://independent-operators.com"
        }
    ],
    "support": {
	"issues": "https://github.com/tarioch/eveapi-fetcher-bundle/issues"
    },
    "require": {
        "php": ">=5.4.4",
	    "symfony/framework-bundle": ">=2.3",
	    "jms/di-extra-bundle" : ">=1.4",
	    "evelabs/phealng-bundle" : "dev",
	    "doctrine/orm" : ">=2.2.3",
	    "doctrine/doctrine-bundle": ">=1.2",
	    "symfony/monolog-bundle": ">=2.3",
	    "mmoreram/gearman-bundle": ">=2.3",
	    "doctrine/doctrine-migrations-bundle": ">=1.0",
	    "doctrine/migrations": ">=1.0@dev"
    },
    "require-dev": {
	    "phpunit/phpunit" : "~4.0",
    	"mockery/mockery" : "~0.9",
	    "phpmd/phpmd" : "~2.0",
    	"squizlabs/php_codesniffer" : "~2.0",
	    "doctrine/doctrine-fixtures-bundle": "dev-master",
    	"liip/functional-test-bundle": "~1.0"
    },
    "autoload": {
	    "psr-0": { "Tarioch\\EveapiFetcherBundle": "" }
    },
    "target-dir": "Tarioch/EveapiFetcherBundle"
}
